    "@types/lodash": "^4.14.171",
    "@types/lodash-es": "^4.17.4",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.1.5",
    "@vueuse/core": "^5.2.0",
    "@vueuse/head": "^0.6.0",
    "compression": "^1.7.4",
