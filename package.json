{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": "> 1%, not dead",
  "scripts": {
    "dev": "node scripts/dev",
    "start": "node scripts/start",
    "static": "node scripts/prerender",
    "build": "yarn build:client && yarn build:server && yarn build:static",
    "build:static": "vite build --outDir dist/static --ssrManifest",
